{
  "check-coverage": true,
  "branches": 100,
  "lines": 100,
  "functions": 100,
  "statements": 100,
  "watermarks": {
    "lines": [85, 95],
    "functions": [90, 95],
    "branches": [85, 95],
    "statements": [85, 95]
  },
  "include": [
    "**"
  ],
  "exclude": [
    "coverage",
    "**/*.spec.*"
  ],
  "report-dir": "./coverage",
  "reporter": [
    "text",
    "lcov"
  ],
  "extension": [
    ".ts"
  ],
  "skip-full": false,
  "all": true,
  "temp-dir": "./.nyc_output"
}